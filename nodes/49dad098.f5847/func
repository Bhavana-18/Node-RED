const LATITUDE = 51.508;
const LONGITUDE = -0.239;
const BASE_URL = 'https://api.darksky.net/forecast/';
const KEY = '7c4f4781e80f09e9f4dc745c01160d92';
const CONFIG = '?units=si&exclude=minutely,hourly,daily,alerts,flags';

var timestamp = msg.payload;
timestamp = Math.round(timestamp/1000);

var params = KEY + '/' +
    LATITUDE + ',' +
    LONGITUDE + ',' +
    timestamp;

msg.url = BASE_URL + params + CONFIG;

return msg;