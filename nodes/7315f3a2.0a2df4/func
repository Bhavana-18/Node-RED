const scenes = [
    'Savanna sunset',
    'Tropical twilight',
    'Ants night',
    'Ants feeding'];

var count = context.get('count');
if (count === undefined) {
    count = 0;
}
if (flow.get('anyOn') === true) {
    if (++count >= scenes.length) {
        count = 0;
    }
}

context.set('count', count);
msg.payload = scenes[count];

node.log('[Fred\'s] set scene #' + count + ' ' + scenes[count]);

return msg;