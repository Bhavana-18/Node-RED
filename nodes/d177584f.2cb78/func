const UNKNOWN = 'Unknown face';

var idPseudos = msg.payload.idPseudos;
for(i=0, n=ids.length; i<n; i++) {
    var pseudo = idPseudos[ids[i]];
    if (pseudo !== undefined) {
        pseudos.push(pseudo);
    }
}
if (pseudos.length === 0) {
    pseudos.push(UNKNOWN);
}

msg.payload = pseudos;

return msg;