var text = '';
var pseudos = msg.payload;
if (pseudos.length === 0) {
    text = 'nobody\'s';
} else {
    pseudos.sort();
    for(i=0; i<pseudos.length; i++) {
        if (text !== '') {
            if (i === pseudos.length-1) {
                text += ' and ';
            } else {
                text += ', ';
            }
        }
        text += pseudos[i];
    }
    text += ' ';
    text += pseudos.length === 1?'is': 'are';
}
text += ' home';

msg.payload = text;

return msg;