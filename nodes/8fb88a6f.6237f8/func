const NB_EVENTS = 100;

var url = 'https://api.netatmo.com/api/geteventsuntil';
var bearer = 'Bearer ' + flow.get('access_token');
var homeId = flow.get('homeId');
var eventId = flow.get('lastEventId');

msg.url = url;
msg.headers = { authorization: bearer };
msg.payload = { home_id: homeId, event_id: eventId, size: NB_EVENTS };

return msg;