const KNOWN_EVENTS = [
    ['topology_changed'     , undefined         , 'topology changed'    ],
    ['webhook_activation'   , undefined         , 'webhook activated'   ],
    ['NACamera-movement'    , 'movement'        , 'topology changed'    ],
    ['NACamera-person'      , 'person'          , 'topology changed'    ],
    ['NACamera-connection'  , 'connection'      , 'connected'           ],
    ['NACamera-connection'  , 'disconnection'   , 'disconnected'        ]
];

var pushType = msg.payload.push_type;
var eventType = msg.payload.event_type;

for(var i=0; i<KNOWN_EVENTS.length; i++) {
    if (KNOWN_EVENTS[i][0] === pushType &&
            KNOWN_EVENTS[i][1] === eventType) {
        node.log('[Welcome] ' + KNOWN_EVENTS[i][2]);
        return null;
    }
}

node.log('[Welcome] unknown event! (' + pushType + '|' + eventType + ')');

return msg;